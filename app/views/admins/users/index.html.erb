<h2>Users</h2>

<% if @users.any? %>
<ul>
  <% @users.each do |user| %>
  <li>
    name: <%= user.name %><br>
    created at: <%= user.created_at.to_s %>
    <%= link_to admins_user_path(user), method: :delete, data: { confirm: 'Sure?' } do %>
      <button><span>Delete</span></button>
    <% end %>
  </li>
  <% end %>
</ul>
<% end %>